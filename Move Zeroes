class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        auto p1 = nums.begin();
        auto p2 = nums.begin();
        
        while (p1 < nums.end() )
        {
            if (*p1 != 0 )
            {
                swap (*p2, *p1);
                p2 ++;
            }
           p1 ++; 
            
        }
        
    }
};
